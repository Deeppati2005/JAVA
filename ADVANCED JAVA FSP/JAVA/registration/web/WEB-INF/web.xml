<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signup2</servlet-name>
        <servlet-class>Signup2.Signup2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup3</servlet-name>
        <servlet-class>signup3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fetch</servlet-name>
        <servlet-class>fetch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginvalidate</servlet-name>
        <servlet-class>loginvalidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fp2</servlet-name>
        <servlet-class>fp2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fp3</servlet-name>
        <servlet-class>fp3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fp4</servlet-name>
        <servlet-class>fp4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dynamicfetch</servlet-name>
        <servlet-class>dynamicfetch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signup2</servlet-name>
        <url-pattern>/Signup2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup3</servlet-name>
        <url-pattern>/signup3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fetch</servlet-name>
        <url-pattern>/fetch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginvalidate</servlet-name>
        <url-pattern>/loginvalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fp2</servlet-name>
        <url-pattern>/fp2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fp3</servlet-name>
        <url-pattern>/fp3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fp4</servlet-name>
        <url-pattern>/fp4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dynamicfetch</servlet-name>
        <url-pattern>/dynamicfetch</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
